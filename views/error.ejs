<!-- that file to get errors messages from express middleware handling error and print it to users -->

<% layout('layouts/boilerplate') %>
<div class="row">
    <div class="col-6 offset-3">
        <div class="alert alert-danger" role="alert">
            <h4 class="alert-heading"><%= err.message %></h4>
           <p><%= err.stack %></p>
          </div>
    </div>
</div>
